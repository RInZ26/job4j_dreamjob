<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>PostServlet</servlet-name>
    <servlet-class>ru.job4j.dream.servlet.post.PostEditServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CandidateEditServlet</servlet-name>
    <servlet-class>ru.job4j.dream.servlet.candidate.CandidateEditServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PostServlet</servlet-name>
    <url-pattern>/post/save</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CandidateEditServlet</servlet-name>
    <url-pattern>/candidate/save</url-pattern>
  </servlet-mapping>


</web-app>
